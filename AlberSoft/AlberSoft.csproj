<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows10.0.19041.0</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
	<WinFormsDefaultFont>Microsoft Sans Serif, 9pt</WinFormsDefaultFont>
	<HighDpiMode>PerMonitorV2</HighDpiMode>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="actividadesRecreativas\**" />
    <Compile Remove="Resources\**" />
    <EmbeddedResource Remove="actividadesRecreativas\**" />
    <EmbeddedResource Remove="Resources\**" />
    <None Remove="actividadesRecreativas\**" />
    <None Remove="Resources\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CuoreUI.Winforms" Version="2025.12.14" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>